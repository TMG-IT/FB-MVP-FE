{
  "defaultSeverity": "error",
  "extends": ["tslint-react"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
